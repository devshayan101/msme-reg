{{!-- Form Starts --}}

<div class="container">
  <div>
    <h2 class="head">
      <center><b>
          {{formHead}}
        </b></center>
    </h2>
  </div>
  <h5 class="submit">
    <center>
      {{msg}}
    </center>
  </h5>
  <form action="/submit" method="POST" enctype="multipart/form-data">
    <div class="form">
      <div class="form-item" id="formType">
        <label for="fromtype" class="label">Form Type</label>
        <select name="formtype" class="input" id="">
          <option value={{formHead}}>FormType</option>

        </select>
      </div>

      <div class="form-item">
        <label for="aadhaarCardName" class="label">Your Name on Aadhaar Card</label>
        <input type="text" class="input" name="aadhaarCardName" placeholder="Your Name" autocomplete="off" required>
      </div>

      <div class="form-item">
        <label for="aadhaarCardNo" class="label">Aadhaar Card Number</label>
        <input type="text" class="input" name="aadhaarCardNo" placeholder="Aadhaar No" autocomplete="off" required>
      </div>

      <div class="form-item">
        <label for="panNumber" class="label">PanCard Number</label>
        <input type="text" class="input" name="panNumber" placeholder="Pan Number" required autocomplete="off">
      </div>

      <div class="form-item">
        <label for="email" class="label">Email-Id</label>
        <input type="email" class="input" name="email" placeholder="Email-Id" required autocomplete="off">
      </div>

      <div class="form-item">
        <label for="phone" class="label">Phone Number</label>
        <input type="text" class="input" name="phone" placeholder="Phone Number" id="" required autocomplete="off">
      </div>

      <div class="form-item">
        <label for="category" class="label">Category</label>
        <select name="category" class="input" id="">
          <option value="general">General</option>
          <option value="sc">SC</option>
          <option value="st">ST</option>
          <option value="obc">OBC</option>
        </select>
      </div>

      <div class="form-item">
        <label for="gender" class="label">Gender</label>

        <select name="gender" id="" class="input">
          <option value="Male">Male</option>
          <option value="Female">Female</option>
        </select>
      </div>

      <div class="form-item">
        <label for="firmName" class="label">Buisness Name</label>
        <input type="text" name="firmName" class="input" placeholder="Buisness Name" required>
      </div>

      <div class="form-item">
        <label for="buisnessType" class="label">Select your business type:</label>

        <select name="buisnessType" class="input" id="">
          <option value="Proprietorship">Proprietorship</option>
          <option value="partnership">Partnership Firm</option>
          <option value="private-limited">Private Limited Company</option>
          <option value="public-limited">Public Limited Company</option>
          <option value="opc">One Person Company</option>
          <option value="llc">Limited Liablity company</option>
          <option value="society-trust-club-AOP">Society/Trust/Club/AOP</option>
          <option value="government-dept">Government Department</option>
        </select>
      </div>

      <div class="form-item">
        <label for="buisnessAddress" class="label">Buisness Address</label>
        <textarea placeholder="This address would be used by banks, etc." name="buisnessAddress" class="input" cols="40"
          rows="4" required> </textarea>
      </div>

      <div class="form-item">
        <label for="dateOfStart" class="label">Date of starting</label>
        <input type="date" name="dateOfStart" class="input date">
      </div>

      <div class="form-item">
        <label for="nature" class="label">Nature of buisness:</label>
        <textarea name="nature"
          placeholder="Sector or products in which you deal. Example- Wood products, Bricks, Plactics, etc." cols="30"
          rows="4" class="input"></textarea>
      </div>

      <div class="form-item">
        <label for="employees" class="label">Number of employees</label>
        <input type="text" name="employees" class="input" placeholder=>
      </div>

      <div class="form-item">
        <label for="investment" class="label">Your investment</label>
        <input type="text" name="investment" class="input" placeholder="Company evaluation">
      </div>

      <div class="form-item">
        <label for="accountNo" class="label">Account Number used in buisness</label>
        <input type="text" name="accountNo" class="input" placeholder="100% Secure">
      </div>

      <div class="form-item">
        <label for="ifsc" class="label">IFSC Code</label>
        <input type="text" name="ifsc" placeholder="On your Checkbook or bank Passbook." class="input">
      </div>

      <div class="form-item">
        <label for="aadhaarFile01" class="label">Upload your Aadhaar Card [FRONT]:</label>
        <input type="file" name="aadhaarFile01" class="input" accept=".jpg, .pdf" required>
      </div>

      <div class="form-item">
        <label for="aadhaarFile02" class="label">Upload your Aadhaar Card [BACK]:</label>
        <input type="file" name="aadhaarFile02" class="input" accept=".jpg, .pdf" required>
      </div>


      <div class="form-item">
        <label for="panFile" class="label">Upload your Pancard:</label>
        <input type="file" id="pan-File" name="panFile" class="input" accept=".jpg, .pdf" required>
      </div>
    </div>

    <input type="reset" value="Clear Form" class="btn btn-primary btn-form">
    <input type="submit" value="Submit" class="btn btn-primary btn-form">


  </form>
</div>
{{!-- Form Ends --}}

{{!-- Process --Starts-- --}}

<div class="process-bucket">

  <center>
    <h3>{{formHead}} Fast and Easy..!!</h3> <br>
  </center>
  <div class="card-deck">
    <div class="card">


      <div class="card-body">
        <h5 class="card-title">
          <center><i class="fas fa-id-card"></i> </center>
        </h5>
        <p class="card-text">Fill the above MSME application form</p>

      </div>
      <center><i class="fas fa-arrow-alt-circle-right new"></i></center>
    </div>
    <div class="card">

      <div class="card-body">
        <h5 class="card-title">
          <center><i class="far fa-credit-card"></i></center>
        </h5>
        <p class="card-text">Pay registration fees to confirm application</p>

      </div>
      <center><i class="fas fa-arrow-alt-circle-right new"></i></center>
    </div>
    <div class="card">

      <div class="card-body">
        <h5 class="card-title">
          <center><i class="fas fa-users"></i>
        </h5>
        </center>
        <p class="card-text">Our executive will process your application</p>

      </div>
      <center><i class="fas fa-arrow-alt-circle-right new"></i></center>
    </div>
    <div class="card">

      <div class="card-body">
        <h5 class="card-title">
          <center><i class="fas fa-envelope"></i></center>
        </h5>
        <p class="card-text">Certificate will be sent to registered e-mail id</p>

      </div>
    </div>
  </div>

</div>

